{
    "applications": {
        "gecko": {
            "id": "castkodi@regseb.github.io",
            "strict_min_version": "95.0"
        }
    },
    "manifest_version": 2,
    "name": "Cast Kodi",
    "version": "6.2.1",
    "description": "__MSG_manifest_description__",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "96": "img/icon96.png",
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "img/icon16.png",
            "24": "img/icon24.png",
            "32": "img/icon32.png",
            "64": "img/icon64.png"
        },
        "default_title": "Cast Kodi",
        "default_popup": "popup/index.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+K"
            }
        }
    },
    "author": "Sébastien Règne",
    "background": {
        "page": "background/index.html"
    },
    "default_locale": "en",
    "homepage_url": "https://github.com/regseb/castkodi",
    "minimum_chrome_version": "96",
    "optional_permissions": [
        "bookmarks",
        "history"
    ],
    "options_ui": {
        "browser_style": false,
        "page": "options/index.html"
    },
    "permissions": [
        "<all_urls>",
        "activeTab",
        "contextMenus",
        "notifications",
        "storage"
    ]
}
