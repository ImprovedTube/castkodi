{
    "name": "castkodi",
    "version": "5.14.0",
    "description": "WebExtension pour diffuser des vidéos / musiques sur Kodi.",
    "keywords": [
        "cast",
        "kodi",
        "webextension",
        "firefox",
        "youtube",
        "twitch",
        "vimeo",
        "soundcloud"
    ],
    "homepage": "https://addons.mozilla.org/addon/castkodi/",
    "bugs": {
        "url": "https://github.com/regseb/castkodi/issues"
    },
    "license": "MIT",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "funding": "https://www.paypal.me/sebastienregne",
    "repository": "regseb/castkodi",
    "type": "module",
    "scripts": {
        "postinstall": "node .script/sync.js",
        "lint": "metalint",
        "lint:types": "tsc",
        "test": "npm run test:unit && npm run test:integration",
        "test:unit": "c8 mocha -r test/polyfill.js test/unit/",
        "test:integration": "mocha -r test/polyfill.js test/integration/",
        "coverage:html": "c8 report --reporter html",
        "coverage:lcov": "c8 report --reporter lcovonly",
        "jsdocs": "typedoc --tsconfig tsconfig_typedoc.json --out jsdocs/ src/",
        "start": "web-ext run -s src/",
        "build": "web-ext build -s src/ -a build/ -o",
        "release": "standard-version",
        "clean": "node .script/clean.js"
    },
    "dependencies": {
        "dialog-polyfill": "0.5.6"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/eslint-parser": "^7.14.7",
        "@babel/eslint-plugin": "^7.14.5",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-syntax-top-level-await": "^7.14.5",
        "@types/firefox-webext-browser": "^82.0.0",
        "@types/jsdom": "^16.2.12",
        "@types/mocha": "^8.2.2",
        "@types/node-fetch": "^2.5.10",
        "@types/sinon": "^10.0.2",
        "addons-linter": "^3.9.0",
        "c8": "^7.7.3",
        "david": "^12.0.0",
        "eslint": "^7.29.0",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsdoc": "^35.4.1",
        "eslint-plugin-mocha": "^9.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.0",
        "eslint-plugin-unicorn": "^34.0.1",
        "htmlhint": "^0.15.1",
        "jsdom": "^16.6.0",
        "jsonlint-mod": "^1.7.6",
        "markdownlint": "^0.23.1",
        "metalint": "^0.9.0",
        "mocha": "^9.0.1",
        "mock-socket": "^9.0.3",
        "node-fetch": "^2.6.1",
        "purgecss": "^4.0.3",
        "sinon": "^11.1.1",
        "standard-version": "^9.3.0",
        "stylelint": "^13.13.1",
        "stylelint-order": "^4.1.0",
        "typedoc": "^0.21.2",
        "typescript": "^4.3.5",
        "web-ext": "^6.2.0",
        "yaml-lint": "^1.2.4"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
