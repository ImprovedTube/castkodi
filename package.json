{
    "name": "castkodi",
    "version": "6.4.1",
    "description": "WebExtension pour diffuser des vidéos / musiques sur Kodi.",
    "keywords": [
        "cast",
        "kodi",
        "webextension",
        "chrome",
        "firefox",
        "youtube",
        "twitch",
        "vimeo",
        "soundcloud"
    ],
    "homepage": "https://github.com/regseb/castkodi#readme",
    "bugs": {
        "url": "https://github.com/regseb/castkodi/issues",
        "email": "regseb@gmail.com"
    },
    "license": "MIT",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "funding": "https://www.paypal.me/sebastienregne",
    "repository": "regseb/castkodi",
    "type": "module",
    "scripts": {
        "postinstall": "node .script/sync.js",
        "lint": "metalint",
        "lint:types": "tsc --project .tsconfig_lint.json",
        "test": "npm run test:unit && npm run test:integration",
        "test:unit": "c8 mocha test/unit/",
        "test:integration": "mocha -t 0 test/integration/",
        "coverage:html": "c8 report --reporter html",
        "coverage:lcov": "c8 report --reporter lcovonly",
        "jsdocs": "typedoc --tsconfig .tsconfig_jsdocs.json --out jsdocs/ src/",
        "start:chromium": "web-ext run -t chromium -s src/",
        "start:firefox": "web-ext run -t firefox-desktop -s src/",
        "build": "node .script/build.js",
        "release": "standard-version",
        "clean": "node .script/clean.js"
    },
    "dependencies": {
        "webextension-polyfill": "0.9.0"
    },
    "devDependencies": {
        "@types/firefox-webext-browser": "94.0.1",
        "@types/jsdom": "16.2.14",
        "@types/mocha": "9.1.0",
        "@types/sinon": "10.0.11",
        "addons-linter": "5.0.0",
        "c8": "7.11.0",
        "eslint": "8.13.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsdoc": "39.0.1",
        "eslint-plugin-mocha": "10.0.3",
        "eslint-plugin-no-unsanitized": "4.0.1",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-promise": "6.0.0",
        "eslint-plugin-regexp": "1.6.0",
        "eslint-plugin-unicorn": "42.0.0",
        "htmlhint": "1.1.3",
        "jsdom": "19.0.0",
        "jsonlint-mod": "1.7.6",
        "markdownlint": "0.25.1",
        "metalint": "0.10.0",
        "mocha": "9.2.2",
        "mock-socket": "9.1.2",
        "node-fetch": "3.2.3",
        "purgecss": "4.1.3",
        "sinon": "13.0.1",
        "standard-version": "9.3.2",
        "stylelint": "14.6.1",
        "stylelint-order": "5.0.0",
        "typedoc": "0.22.14",
        "typescript": "4.6.3",
        "web-ext": "6.8.0",
        "yaml-lint": "1.2.4"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "private": true
}
